{
  "cleanUrls": true,
  "headers": [
    {
      "source": "/blog/2022/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000, public, must-revalidate"
        }
      ]
    },
    {
      "source": "/blog/2023/02/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000, public, must-revalidate"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, must-revalidate"
        }
      ]
    },
    {
      "source": "/_vercel/insights/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=1209600, public, must-revalidate"
        }
      ]
    },
    {
      "source": "/scripts/pic/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, must-revalidate"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/privacy",
      "destination": "mailto:ben@kitia.net?subject=Privacy%20inquiry",
      "permanent": false
    },
    {
      "source": "/writings/:path*",
      "destination": "/blog/:path*",
      "permanent": false
    }
  ],
  "trailingSlash": false
}
