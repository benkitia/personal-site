{
  "cleanUrls": true,
  "headers": [
    {
      "source": "/blog/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=900, public, immutable"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3888000, immutable"
        }
      ]
    },
    {
      "source": "/_vercel/insights/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=604800, public, immutable"
        }
      ]
    },
    {
      "source": "/scripts/pic/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/email",
      "destination": "mailto:ben@kitia.net",
      "permanent": false
    },
    {
      "source": "/privacy",
      "destination": "mailto:ben@kitia.net?subject=Privacy%20inquiry",
      "permanent": false
    },
    {
      "source": "/writings/:path*",
      "destination": "/blog/:path*",
      "permanent": false
    }
  ],
  "trailingSlash": false
}
